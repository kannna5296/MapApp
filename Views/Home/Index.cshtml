@{
    ViewData["Title"] = "Home Page";
}
<div class="row">
    <div class="col-md-3">
        <div class="mb-md-5">
            <p class="heading font-weight-bold">地点検索</p>
            <input id="address" type="text" placeholder="地点を入力してください" />
            <button id="getMap" class="btn">Get Map</button>
        </div>
        <div class="mb-md-5">
            <p class="heading font-weight-bold">近隣コワーキングスペース検索</p>
            <button id="getCW" class="btn">コワーキングスペース表示</button>
        </div>
        <div class="mb-md-5">
            <p class="heading font-weight-bold">経路検索</p>
            <div>
                <input id="start" type="text" placeholder="始点を入力してください" />
            </div>
            <div>
                <input id="end" type="text" placeholder="終点を入力してください" />
            </div>
            <div>
                移動方法
                <select id="travelmode">
                    <option value="DRIVING">車</option>
                    <option value="WALKING">徒歩</option>
                    <option value="BICYCLING">自転車</option>
                    <option value="TRANSIT">電車</option>
                </select>
            </div>
            <button id="getRoute" class="btn" value="Get Route">Get Route</button>
        </div>
    </div>
    <div class="col-md-6">
        <div id="map"></div>
    </div>
    <div class="col-md-3">
        <p>CW詳細A</p>
        <p id="cw1_name">AAA_名前</p>
        <p id="cw1_description">AAA_説明</p>
        <p>CW詳細B</p>
        <p id="cw2_name">BBB_名前</p>
        <p id="cw2_description">BBB_説明</p>
        <p>CW詳細C</p>
        <p id="cw3_name">CCC_名前</p>
        <p id="cw3_description">CCC_説明</p>
    </div>

</div>